<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.spzx.product.mapper.ProductDetailsMapper">

    <select id="selectByProductId" resultType="com.atguigu.spzx.model.entity.product.ProductDetails">
        select * from product_details where id=#{product_id} and is_deleted=0
    </select>
</mapper>